<h1>Listing last_hours</h1>

<table>
  <tr>
    <th>Resolved url</th>
    <th>Count</th>
  </tr>

<% @last_hours.each do |last_hour| %>
  <tr>
    <td><%=h last_hour.resolved_url %></td>
    <td><%=h last_hour.count %></td>
    <td><%= link_to 'Show', last_hour %></td>
    <td><%= link_to 'Edit', edit_last_hour_path(last_hour) %></td>
    <td><%= link_to 'Destroy', last_hour, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New last_hour', new_last_hour_path %>